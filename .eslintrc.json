{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "func-names": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "max-len": ["error", {
      "code": 100,
      "ignorePattern": "(`|')[^\\1]{70,}\\1"
    }]
  }
}
